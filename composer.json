{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "benfranke/gdax-php": "^0.0.6",
        "box/spout": "^2.7",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "laravel-notification-channels/telegram": "^0.0.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Amavis442\\Trading\\": "packages/amavis442/trading/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Amavis442\\Trading\\Tests\\": "packages/amavis442/trading/tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "lint": "phpcs -p --standard=psr2 packages/amavis442/trading/src",
        "phpunit": "phpunit",
        "phpunit-integration": "phpunit --testsuite Integration",
        "phpunit-unit": "phpunit --testsuite Unit --testsuit TraderUnit",
        "phpunit-coverage": "phpunit --coverage-clover=coverage.clover",
        "test": [
            "@lint",
            "@phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
